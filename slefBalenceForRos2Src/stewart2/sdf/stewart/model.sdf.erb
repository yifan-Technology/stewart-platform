        <namespace>/stewart</namespace>
        <argument>set_joint_trajectory:=set_trajectory_stewart</argument>
      </ros>
      <update_rate>100</update_rate>
    </plugin>

        <plugin name="stewart_billot_joint_state_publisher"
          filename="libgazebo_ros_joint_state_publisher.so">
