	<link name = "base_link" >
		<pose> 0 0 <%= BASE_HEIGHT/2 %> 0 0 0 </pose>
		<inertial>
			<mass> <%= BASE_MASS %> </mass>
			<inertia>
				<ixx> <%= BASE_MASS*((BASE_HEIGHT**2)/12.0+(BASE_RADIUS**2)/4) %> </ixx>
				<ixy> 0.0 </ixy>
				<ixz> 0.0</ixz>
				<iyy> <%= BASE_MASS*((BASE_HEIGHT**2)/12.0+(BASE_RADIUS**2)/4) %> </iyy>
				<iyz> 0.0 </iyz>
				<izz> <%= (BASE_MASS*BASE_RADIUS**2)/2 %> </izz>
			</inertia>
		</inertial>

